{

    "output": null,

    "output.name": "site",


    "title": "设计模式 python",
    "author" : "fbigun",
    "description": "python中各种设计模式的实现方法",
    "language" : "zh-hans",
    "gitbook" : "3.2.3",
    "root": ".",

    "extension": ["epub", "pdf", "mobi"],

    "plugins": ["prism", "autocover", "-highlight", "-lunr", "-search", "search-plus"],

    "pluginsConfig": {
